{
    "settings": {
        "react":{
            "version":"detect"
        }
    },
    "extends": [
        "eslint:recommended",
        "airbnb",
        "plugin/react:recommended"
    ],
    "plugins": ["react"],
    "parserOptions": {
        "project": "./tsconfig.json"
    },"rules": {
        "react/jsx-use-react":"error",
        "react/jsx-use-vars":"error"
    }
}